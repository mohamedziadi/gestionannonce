<div class="container">
<div class="col-md-6">
<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
      <div class="form-group">
                <legend>Reset Password</legend>
              <div class="form-group">
                 <label for="password"> {{ form_label(form.new.first)}}
                 </label>
                 
                 {{ form_widget(form.new.first, {'attr':{'class':'form-control' }}) }}
              </div>
             
             <div class="form-group">
                 <label for="password"> {{ form_label(form.new.second)}}
                 </label>
                 
                 {{ form_widget(form.new.second, {'attr':{'class':'form-control' }}) }}
              </div>
               {{ form_rest(form) }}
         
    
    <div>
        <input type="submit" value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form></div>
    </div>
